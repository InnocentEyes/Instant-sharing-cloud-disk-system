\r\n                 pall[i].classList.remove('active_p')\r\n          }\r\n      },\r\n      //关闭抽屉组件\r\n     handleClose(done) {      \r\n            done();\r\n      },\r\n      //文件列表里的提交搜索按钮\r\n      summit(){\r\n          //发送请求\r\n          \r\n      },\r\n\r\n      //打开抽屉组件，加载数据\r\n      turntopage(){\r\n          //显示抽屉\r\n            this.drawer = true\r\n            //加载数据\r\n            this.getdata();\r\n          \r\n      },\r\n\r\n      getdata(){\r\n          //加载数据函数 判断文件格式对应选择图片url\r\n          \r\n      },\r\n      //上传文件后处理函数\r\n      handleFileUpload(event){\r\n                  event.preventDefault();\r\n                 let filenameshow =document.getElementsByClassName('filenameshow')[0]\r\n                 let child =`\r\n                 <div class=\"loaditem\">\r\n                            <div>\r\n                               <img src=\"../img/load.png\" alt=\"\" >\r\n                               <p>liuyingying.jpg</p>\r\n                               <p>269KB</p>\r\n                            </div>\r\n                            <div class=\"bottom\">\r\n                                <el-progress :percentage=\"percentage\" :color=\"customColors\" ></el-progress>\r\n                            </div>\r\n                           \r\n                        </div>\r\n                         `\r\n                 filenameshow.appendChild(child);\r\n                },\r\n  },\r\n  \r\n  created(){\r\n     \r\n  }\r\n}\r\n</script>\r\n<style >\r\n  .storage{\r\n      position: fixed;\r\n    z-index: 99;\r\n    top: 0;\r\n    bottom: 0;\r\n    left: 0;\r\n    right: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    backdrop-filter: blur(10px);\r\n      background-color: rgba(0, 0, 0, 0.6)\r\n  }\r\n\r\n  .storage .storage_img{\r\n      position: absolute;\r\n      height: 20%;\r\n      top: -20%;\r\n      cursor: pointer;\r\n  }\r\n  .storage .storage_p{\r\n       position: absolute;\r\n       color: #fff;\r\n      font-size: xx-large;\r\n      bottom: 20%;\r\n      padding-left: 35px;\r\n  }\r\n  .active{\r\n    animation:move 2s infinite;\r\n\t-webkit-animation:move 2s infinite; /*Safari and Chrome*/\r\n    animation-iteration-count:1;\r\n    -webkit-animation-iteration-count:1; /*Safari and Chrome*/\r\n   animation-fill-mode:forwards;\r\n  }\r\n  .active_p{\r\n       animation:move_p 2s infinite;\r\n\t-webkit-animation:move_p 2s infinite; /*Safari and Chrome*/\r\n    animation-iteration-count:1;\r\n    -webkit-animation-iteration-count:1; /*Safari and Chrome*/\r\n   animation-fill-mode:forwards;\r\n  }\r\n  \r\n  @keyframes move\r\n{\r\n\tfrom {top: -20%;}\r\n\tto {top: 45%;}\r\n}\r\n\r\n@-webkit-keyframes move /*Safari and Chrome*/\r\n{\r\n\tfrom {top: -20%;}\r\n\tto {top: 45%;}\r\n}\r\n  @keyframes move_p\r\n{\r\n\tfrom {bottom: -20%;}\r\n\tto {bottom: 25%;}\r\n}\r\n\r\n@-webkit-keyframes move_p /*Safari and Chrome*/\r\n{\r\n\tfrom {bottom: -20%;}\r\n\tto {bottom: 25%;}\r\n}\r\n  .storage .storage_img:hover{\r\n      height: 22%;\r\n  }\r\n  .storage img:nth-child(1),\r\n  .storage p:nth-child(6){\r\n      left: 5%;\r\n  }\r\n  .storage img:nth-child(2),\r\n  .storage p:nth-child(7){\r\n      left: 30%;\r\n  }\r\n  .storage img:nth-child(3),\r\n  .storage p:nth-child(8){\r\n      left: 55%;\r\n  }\r\n  .storage img:nth-child(4),\r\n  .storage p:nth-child(9){\r\n      left: 80%;\r\n  }\r\n  \r\n  .storage .close{\r\n      position: absolute;\r\n      height: 6%;\r\n      top: 5%;\r\n      right:5%;\r\n      cursor: pointer;\r\n  }\r\n  .center{\r\n    position: absolute;\r\n    height: 98%;\r\n    width: 94%;\r\n    left: 50%;\r\n    top: 50%;\r\n    transform: translate(-50%,-50%);\r\n    background-color: #fff;\r\n     box-shadow:#000 0px 0px 50px;\r\n     border-radius: 2rem;\r\n}\r\n.head{\r\n    position: absolute;\r\n    height: 12%;\r\n    width: 100%;\r\n    border-bottom: 1px solid#E0E0E0;\r\n} \r\n.head div{\r\n     position: absolute;\r\n    color: #fff;\r\n    cursor: pointe