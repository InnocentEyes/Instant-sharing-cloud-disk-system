c=\"../img/bluemusic.jpg\" alt=\"\">\r\n                        <input type=\"file\" class=\"Field\" ref=\"file\" accept=\".mp3,.mp4\" multiple = 5 v-on:change=\"handleFileUpload($event)\">\r\n                        <p>上传音乐</p>\r\n                    </div>\r\n                    <div>\r\n                        <img src=\"../img/bluevideo.jpg\" >\r\n                        <input type=\"file\" class=\"Field\" alt=\"\" multiple = 3 v-on:change=\"handleFileUpload($event)\">\r\n                        <p>上传视频</p>\r\n                    </div>\r\n                   \r\n                    </div>\r\n                    <div class=\"filenameshow\">\r\n                        <div class=\"loaditem\">\r\n                            <div>\r\n                               <img src=\"../img/load.png\" alt=\"\" >\r\n                               <p>liuyingying.jpg</p>\r\n                               <p>269KB</p>\r\n                            </div>\r\n                            <div class=\"bottom\">\r\n                                <el-progress :percentage=\"percentage\" :color=\"customColors\" ></el-progress>\r\n                            </div>\r\n                           \r\n                        </div>\r\n                        \r\n                        \r\n                    </div>\r\n                    \r\n                </div>\r\n                \r\n            </div>\r\n        </div>\r\n        </el-drawer>\r\n    \r\n</div>\r\n    \r\n</template>\r\n\r\n<script>\r\nimport bus from '../utils/bus'\r\n\r\nexport default {\r\n  name: 'Storage',\r\n \r\n  data(){\r\n      return{\r\n          style:{\r\n              visibility:'hidden'\r\n          },\r\n          //抽屉默认显示\r\n         drawer: false,\r\n         direction:'rtl' ,\r\n         //头像\r\n          circleUrl:\"https://cube.elemecdn.com/3/7c/3ea6beec64369c2642b92c6726f1epng.png\",\r\n           sizeList: [\"large\"],\r\n        //上传，好友，文件列表切换显示\r\n         isShowFileList:false,\r\n         i