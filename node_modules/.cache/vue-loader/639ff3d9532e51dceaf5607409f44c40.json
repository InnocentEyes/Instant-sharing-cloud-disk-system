         <!-- 点击文件列表 -->\r\n          <div class=\"FileList\" v-show=\"isShowFileList\">\r\n            <div class=\"leftbox\">\r\n              <ul>\r\n                <li class=\"allfile\"><a>全部文件</a></li>\r\n                <li><a>图片</a></li>\r\n                <li><a>视频</a></li>\r\n                <li><a>音乐</a></li>\r\n                <li><a>文档</a></li>\r\n              </ul>\r\n            </div>\r\n            <div class=\"search\">\r\n              <el-input placeholder=\"请输入内容\" class=\"input-with-select\">\r\n                <el-button\r\n                  slot=\"append\"\r\n                  icon=\"el-icon-search\"\r\n                  @click=\"summit\"\r\n                ></el-button>\r\n              </el-input>\r\n            </div>\r\n\r\n            <div class=\"viewFileList\">\r\n              <div class=\"item\" v-for=\"(i, index) in list\" :key=\"index + 1\">\r\n                <img :src=\"i.url\" alt=\"\" class=\"showcover\" />\r\n                <p class=\"showname\">{{ i.name }}</p>\r\n                <button class=\"download\">下载</button>\r\n                <button class=\"look\">查看</button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <!-- 点击好友列表 -->\r\n          <div class=\"FriendList\"></div>\r\n          <!-- 点击上传 -->\r\n          <div>\r\n            <div class=\"LoadUp\" v-show=\"isShowLoadUp\">\r\n              <div>\r\n                <img src=\"../img/blueimg.jpg\" alt=\"\" />\r\n                <input\r\n                  type=\"file\"\r\n                  class=\"Field\"\r\n                  accept=\".jpg,.png\"\r\n                  multiple=\"5\"\r\n                  v-on:change=\"handleFileUpload($event)\"\r\n                />\r\n                <p>上传图片</p>\r\n              </div>\r\n              <div>\r\n                <img src=\"../img/bluefile.jpg\" alt=\"\" />\r\n                <input\r\n                  type=\"file\"\r\n                  class=\"Field\"\r\n                  accept=\".docx,.txt,.pdf,.doc,.ppt,.pptx,.xls,.xlsx\"\r\n                  multiple=\"5\"\r\n                  v-on:change=\"handleFileUpload($event)\"\r\n                />\r\n                <p>上传文件</p>\r\n              </div>\r\n              <div>\r\n                <img src=\"../img/bluemusic.jpg\" alt=\"\" />\r\n                <input\r\n                  type=\"file\"\r\n                  class=\"Field\"\r\n                  ref=\"file\"\r\n                  accept=\".mp3\"\r\n                  multiple=\"5\"\r\n                  v-on:change=\"handleFileUpload($event)\"\r\n                />\r\n                <p>上传音乐</p>\r\n              </div>\r\n              <div>\r\n                <img src=\"../img/bluevideo.jpg\" />\r\n                <input\r\n                  type=\"file\"\r\n                  class=\"Field\"\r\n                  alt=\"\"\r\n                  accept=\".mp4,.flv,.swf,.ogg,.webm\"\r\n                  multiple=\"3\"\r\n                  v-on:change=\"handleFileUpload($event)\"\r\n                />\r\n                <p>上传视频</p>\r\n              </div>\r\n            </div>\r\n            <div class=\"filenameshow\">\r\n              <div\r\n                class=\"loaditem\"\r\n                v-for=\"(i, index) in fileslist\"\r\n                :key=\"index + 1\"\r\n              >\r\n                <div>\r\n                  <img src=\"../img/load.png\" alt=\"\" />\r\n                  <p>{{ i.name }}</p>\r\n                  <p>{{ i.size }}</p>\r\n                </div>\r\n                <div class=\"bottom\">\r\n                  <el-progress\r\n                    :percentage=\"percentage\"\r\n                    :color=\"customColor\"\r\n                  ></el-progress>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </el-drawer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport bus from \"../utils/bus\";\r\nimport axios from \"axios\";\r\nimport { Loading } from 'element-ui'\r\nimport Video from \"../components/Video.vue\"\r\nimport Audio from \"../components/Audio.vue\"\r\nexport default {\r\n  name: \"Storage\",\r\n  components:{\r\n    Video,\r\n    Audio\r\n  },\r\n  data() {\r\n    return {\r\n      style: {\r\n        visibility: \"hidden\",\r\n      },\r\n      //抽屉默认显示\r\n      drawer: false,\r\n      direction: \"rtl\",\r\n      //头像\r\n      circleUrl:\r\n        \"https://cube.elemecdn.com/3/7c/3ea6beec64369c2642b92c6726f1epng.png\",\r\n      sizeList: [\"large\"],\r\n      //上传，好友，文件列表切换显示\r\n      isShowFileList: true,\r\n      isShowFriendList: false,\r\n      isShowLoadUp: false,\r\n      //上传列表\r\n      fileslist: [],\r\n      //进度条显示\r\n      percentage: 0,\r\n      customColor: \"#409eff\",\r\n      //获取好友列表信息\r\n\r\n      //获取全部文件的信息\r\n      list: [\r\n        {\r\n          url: require(\"../img/musicshow.png\"),\r\n          name: \"111111111111111111111\",\r\n        },\r\n        {\r\n          url: require(\"../img/musicshow.png\"),\r\n          name: \"1111111111111111111111111\",\r\n        },\r\n        {\r\n          url: require(\"../img/musicshow.png\"),\r\n          name: \"1111111111111111111111111\",\r\n        },\r\n        {\r\n          url: require(\"../img/musicshow.png\"),\r\n          name: \"1111111111111111111111111111\",\r\n        },\r\n        {\r\n          url: require(\"../img/musicshow.png\"),\r\n          name: \"11111111111111111111111111111\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  mounted() {\r\n    bus.$off(\"event\"); //阻止事件被触发多次\r\n    bus.$on(\"event\", this.show); //绑定\r\n  },\r\n  computed: {},\r\n  methods: {\r\n    //显示遮罩层\r\n    show(val) {\r\n      let imgall = document.getElementsByClassName(\"storage_img\");\r\n      let pall = document.getElementsByClassName(\"storage_p\");\r\n      this.style.visibility = val;\r\n      for (let i = 0; i < imgall.length; i++) {\r\n        imgall[i].classList.add(\"active\");\r\n        pall[i].classList.add(\"active_p\");\r\n      }\r\n    },\r\n    //关闭遮罩层\r\n    close() {\r\n      this.style.visibility = \"hidden\";\r\n      let imgall = document.getElementsByClassName(\"storage_img\");\r\n      let pall = document.getElementsByClassName(\"storage_p\");\r\n      for (let i = 0; i < imgall.length; i++) {\r\n        imgall[i].classList.remove(\"active\");\r\n        pall[i].classList.remove(\"active_p\");\r\n      }\r\n    },\r\n    //关闭抽屉组件\r\n    handleClose(done) {\r\n      done();\r\n    },\r\n    //文件列表里的提交搜索按钮\r\n    summit() {\r\n      //发送请求\r\n    },\r\n\r\n    //打开抽屉组件，加载数据\r\n    turntopage() {\r\n      this.drawer = true;\r\n      this.getdata();\r\n    },\r\n    //加载全部文件函数\r\n    getdata() {\r\n      // axios({\r\n      //   url: \"https://www.netdisc.cloud/file/getAll\",\r\n      //   method: \"post\",\r\n      //    headers:{\r\n      //     'Content-Type':'application/json'\r\n      //   },\r\n      // })\r\n      //   .then(function (res) {\r\n      //     console.log(res);\r\n      //   })\r\n      //   .catch(function (error) {\r\n      //     //上传失败，弹窗显示失败\r\n      //     console.log(error);\r\n      //   });\r\n    },\r\n    //上传文件后处理函数\r\n    handleFileUpload(event) {\r\n      event.preventDefault();\r\n      let Field = document.getElementsByClassName(\"Field\");\r\n      for (let i = 0; i < Field.length; i++) {\r\n        if (Field[i].files.length != 0) {\r\n          for (let j = 0; j < Field[i].files.length; j++) {\r\n            //添加上传进度条组件\r\n            let name = Field[i].files[j].name;\r\n            let number = Field[i].files[j].size;\r\n            number = this.changeunit(number);\r\n            this.fileslist.push({ name: name, size: number });\r\n            //判断是图片上传还是文件上传还是视频上传还是音乐上传,发送不同请求\r\n            console.log(Field[i].files[j]);\r\n            //单图片请求\r\n            if (\r\n              Field[i].files[j].type == \"image/jpeg\" &&\r\n              Field[i].files.length == 1\r\n            ) {\r\n              this.img_singleload();\r\n            } else if (\r\n              Field[i].files[j].type == \"image/jpeg\" &&\r\n              Field[i].files.length > 1\r\n            ) {\r\n              this.img_multiload();\r\n            }\r\n          }\r\n        }\r\n        Field[i].value = \"\";\r\n      }\r\n    },\r\n    //文件大小size转换单位\r\n    changeunit(number) {\r\n      if (number < 1024) {\r\n        return number + \"bytes\";\r\n      } else if (number > 1024 && number < 1048576) {\r\n        return (number / 1024).toFixed(1) + \"KB\";\r\n      } else if (number > 1048576) {\r\n        return (number / 1048576).toFixed(1) + \"MB\";\r\n      }\r\n    },\r\n    //单张图片上传\r\n    img_singleload() {\r\n      //发送请求 上传成功，弹窗显示成功\r\n      let that = this;\r\n      axios({\r\n        url: \"/img/single/upload\",\r\n        method: \"post\",\r\n        onDownloadProgress(progress) {\r\n          that.percentage = Math.round(\r\n            (progress.loaded / progress.total) * 100\r\n          );\r\n        },\r\n      })\r\n        .then(function (res) {\r\n          that.percentage = 100;\r\n        })\r\n        .catch(function (error) {\r\n          //上传失败，弹窗显示失败\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    //多张图片上传\r\n    img_multiload() {\r\n      //发送请求 上传成功，弹窗显示成功\r\n      let that = this;\r\n      axios({\r\n        url: \"/img/multi/upload\",\r\n        method: \"post\",\r\n        onDownloadProgress(progress) {\r\n          that.percentage = Math.round(\r\n            (progress.loaded / progress.total) * 100\r\n          );\r\n        },\r\n      })\r\n        .then(function (res) {\r\n          that.percentage = 100;\r\n        })\r\n        .catch(function (error) {\r\n          //上传失败，弹窗显示失败\r\n          console.log(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.storage {\r\n  position: fixed;\r\n  z-index: 99;\r\n  top: 0;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n 